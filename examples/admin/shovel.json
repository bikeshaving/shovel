{
  "databases": {
    "main": {
      "dialect": "bun-sqlite",
      "driver": {
        "module": "bun:sqlite",
        "export": "Database"
      },
      "url": "./data/admin.db",
      "schema": "./src/schema.ts"
    }
  },
  "directories": {
    "uploads": {
      "module": "@b9g/filesystem/node-fs.js",
      "export": "NodeFSDirectory",
      "path": "./data/uploads"
    },
    "tmp": {}
  },
  "caches": {
    "sessions": {}
  },
  "logging": {
    "loggers": [
      {"category": "shovel", "level": "debug"}
    ]
  }
}
