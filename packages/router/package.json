{
  "name": "@b9g/router",
  "version": "0.1.0",
  "description": "Universal request router built on web standards",
  "type": "module",
  "scripts": {
    "build": "libuild build",
    "build:save": "libuild build --save",
    "publish": "libuild publish",
    "test": "bun test"
  },
  "keywords": [
    "router",
    "cache-first",
    "web-standards",
    "universal"
  ],
  "dependencies": {
    "@b9g/match-pattern": "file:../match-pattern",
    "@b9g/cache": "file:../cache"
  },
  "devDependencies": {
    "@b9g/libuild": "^0.1.0",
    "bun-types": "latest"
  },
  "private": true,
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index.js": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./router": {
      "types": "./dist/src/router.d.ts",
      "import": "./dist/src/router.js"
    },
    "./router.js": {
      "types": "./dist/src/router.d.ts",
      "import": "./dist/src/router.js"
    },
    "./package.json": "./dist/package.json"
  }
}
