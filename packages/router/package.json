{
  "name": "@b9g/router",
  "version": "0.2.0",
  "description": "Universal request router built on web standards with generator-based middleware.",
  "type": "module",
  "keywords": [
    "router",
    "serviceworker",
    "middleware",
    "universal",
    "web-standards",
    "urlpattern",
    "generator",
    "shovel"
  ],
  "scripts": {
    "build": "libuild build --save",
    "prepublishOnly": "echo 'ERROR: Cannot publish from root directory. Use libuild publish instead.' && exit 1"
  },
  "dependencies": {
    "@b9g/http-errors": "workspace:*",
    "@b9g/match-pattern": "workspace:*"
  },
  "devDependencies": {
    "@b9g/libuild": "^0.1.18"
  },
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index.js": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./middleware": {
      "types": "./dist/src/middleware.d.ts",
      "import": "./dist/src/middleware.js"
    },
    "./middleware.js": {
      "types": "./dist/src/middleware.d.ts",
      "import": "./dist/src/middleware.js"
    },
    "./package.json": "./dist/package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bikeshaving/shovel.git",
    "directory": "packages/router"
  }
}
