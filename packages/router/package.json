{
  "name": "@b9g/router",
  "version": "0.1.10",
  "description": "Universal request router for ServiceWorker applications. Built on web standards with cache-aware routing and generator-based middleware.",
  "type": "module",
  "keywords": [
    "router",
    "serviceworker",
    "middleware",
    "cache",
    "universal",
    "web-standards",
    "urlpattern",
    "shovel"
  ],
  "scripts": {
    "build": "libuild build --save",
    "prepublishOnly": "echo 'ERROR: Cannot publish from root directory. Use libuild publish instead.' && exit 1"
  },
  "dependencies": {
    "@b9g/http-errors": "workspace:*",
    "@b9g/match-pattern": "workspace:*"
  },
  "devDependencies": {
    "@b9g/libuild": "^0.1.18",
    "bun-types": "latest"
  },
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index.js": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./package.json": "./dist/package.json"
  }
}
