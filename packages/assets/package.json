{
  "name": "@b9g/assets",
  "version": "0.1.0",
  "description": "Assets middleware using File System Access API - no ESBuild dependencies",
  "type": "module",
  "scripts": {
    "build": "libuild build",
    "build:save": "libuild build --save",
    "publish": "libuild publish",
    "test": "bun test"
  },
  "keywords": [
    "shovel",
    "assets",
    "middleware",
    "filesystem-api",
    "universal"
  ],
  "dependencies": {
    "@b9g/platform": "file:../platform"
  },
  "devDependencies": {
    "@b9g/libuild": "^0.1.0",
    "bun-types": "latest",
    "@types/node": "^18.0.0"
  },
  "private": true,
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./middleware": {
      "types": "./dist/src/middleware.d.ts",
      "import": "./dist/src/middleware.js"
    },
    "./plugin": {
      "types": "./dist/src/plugin.d.ts",
      "import": "./dist/src/plugin.js"
    },
    "./populate": {
      "types": "./dist/src/populate.d.ts",
      "import": "./dist/src/populate.js"
    },
    "./package.json": "./dist/package.json",
    "./index": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index.js": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./middleware.js": {
      "types": "./dist/src/middleware.d.ts",
      "import": "./dist/src/middleware.js"
    },
    "./plugin.js": {
      "types": "./dist/src/plugin.d.ts",
      "import": "./dist/src/plugin.js"
    },
    "./populate.js": {
      "types": "./dist/src/populate.d.ts",
      "import": "./dist/src/populate.js"
    }
  }
}
