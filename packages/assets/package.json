      "types": "./dist/src/middleware.d.ts",
      "import": "./dist/src/middleware.js"
    },
    "./global.d.ts": "./dist/src/global.d.ts",
    "./shovel-assets.d.ts": "./dist/src/shovel-assets.d.ts"
