{
  "name": "@b9g/http-errors",
  "version": "0.1.5",
  "type": "module",
  "description": "Standard HTTP error responses for ServiceWorker applications. Returns proper Response objects with status codes, not thrown exceptions.",
  "keywords": [
    "http",
    "errors",
    "response",
    "serviceworker",
    "status-codes",
    "web-standards",
    "functional",
    "shovel"
  ],
  "scripts": {
    "build": "libuild build --save",
    "dev": "libuild build --watch",
    "test": "bun test",
    "prepublishOnly": "echo 'ERROR: Cannot publish from root directory. Use libuild publish instead.' && exit 1"
  },
  "dependencies": {},
  "devDependencies": {
    "@b9g/libuild": "^0.1.18",
    "bun-types": "latest"
  },
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./package.json": "./dist/package.json",
    "./index": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index.js": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  }
}
