{
  "name": "@b9g/cache",
  "version": "0.1.0",
  "description": "Universal Cache API implementation",
  "type": "module",
  "scripts": {
    "build": "libuild build",
    "build:save": "libuild build --save",
    "publish": "libuild publish",
    "test": "bun test"
  },
  "keywords": [
    "cache",
    "storage",
    "web-standards",
    "universal"
  ],
  "dependencies": {},
  "devDependencies": {
    "@b9g/libuild": "^0.1.0",
    "bun-types": "latest"
  },
  "private": true,
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
