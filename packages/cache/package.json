    },
    "./memory": {
      "types": "./dist/src/memory.d.ts",
      "import": "./dist/src/memory.js"
    },
    "./postmessage": {
      "types": "./dist/src/postmessage.d.ts",
      "import": "./dist/src/postmessage.js"
    },
    "./storage": {
      "types": "./dist/src/cache-storage.d.ts",
