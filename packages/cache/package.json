{
  "name": "@b9g/cache",
  "version": "0.2.2",
  "description": "Universal Cache API for ServiceWorker applications. Provides standard CacheStorage and Cache interfaces across all JavaScript runtimes.",
  "type": "module",
  "scripts": {
    "build": "libuild build --save",
    "prepublishOnly": "echo 'ERROR: Cannot publish from root directory. Use libuild publish instead.' && exit 1"
  },
  "keywords": [
    "cache",
    "storage",
    "serviceworker",
    "cachestorage",
    "web-standards",
    "universal",
    "shovel"
  ],
  "dependencies": {},
  "devDependencies": {
    "@b9g/libuild": "^0.1.18"
  },
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./memory": {
      "types": "./dist/src/memory.d.ts",
      "import": "./dist/src/memory.js"
    },
    "./postmessage": {
      "types": "./dist/src/postmessage.d.ts",
      "import": "./dist/src/postmessage.js"
    },
    "./package.json": "./dist/package.json",
    "./index": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index.js": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./memory.js": {
      "types": "./dist/src/memory.d.ts",
      "import": "./dist/src/memory.js"
    },
    "./postmessage.js": {
      "types": "./dist/src/postmessage.d.ts",
      "import": "./dist/src/postmessage.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bikeshaving/shovel.git",
    "directory": "packages/cache"
  }
}
