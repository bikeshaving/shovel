{
  "name": "@b9g/admin",
  "private": true,
  "version": "0.1.0",
  "description": "Django-like admin interface for Shovel applications",
  "type": "module",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./package.json": "./dist/package.json",
    "./index": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./index.js": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "libuild build --save",
    "prepublishOnly": "echo 'ERROR: Cannot publish from root directory. Use libuild publish instead.' && exit 1"
  },
  "keywords": [
    "admin",
    "crud",
    "shovel"
  ],
  "author": "b9g",
  "license": "MIT",
  "dependencies": {
    "@b9g/crank": "^0.7.2",
    "@b9g/oauth2": "workspace:*",
    "@b9g/router": "workspace:*",
    "@uswds/uswds": "^3.13.0"
  },
  "peerDependencies": {
    "@b9g/zen": "^0.1.6",
    "@b9g/platform": "workspace:*"
  },
  "devDependencies": {
    "@b9g/zen": "^0.1.6",
    "@b9g/libuild": "^0.1.18",
    "@types/bun": "^1.2.2",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bikeshaving/shovel.git",
    "directory": "packages/admin"
  }
}
