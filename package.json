{
  "name": "@b9g/shovel",
  "version": "0.1.10",
  "description": "The obsessively web platform-based web framework",
  "license": "MIT",
  "type": "module",
  "bin": {
    "shovel": "./dist/src/cli.js"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "libuild build",
    "test": "node --experimental-fetch node_modules/.bin/uvu"
  },
  "dependencies": {
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@b9g/crank": "^0.6.0",
    "@b9g/libuild": "^0.1.8",
    "@emotion/css": "^11.13.5",
    "@flydotio/dockerfile": "^0.7.8",
    "@types/bun": "^1.2.2",
    "fkill": "^9.0.0",
    "sinon": "^19.0.2",
    "typescript": "^5.7.3",
    "uvu": "^0.5.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": true,
  "module": "./dist/src/cli.js",
  "types": "./dist/src/cli.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/cli.d.ts",
      "import": "./dist/src/cli.js"
    },
    "./cli": {
      "types": "./dist/src/cli.d.ts",
      "import": "./dist/src/cli.js"
    },
    "./cli.js": {
      "types": "./dist/src/cli.d.ts",
      "import": "./dist/src/cli.js"
    },
    "./package.json": "./dist/package.json"
  }
}
